{
    "name": "02-samples",
    "version": "1.0.0",
    "description": "Ejemplos typescript",
    "main": "build/main.js",
    "scripts": {
        "build": "tsc",
        "start": "node build/main.js",
        "start:dev": "ts-node src/main.ts --inspect=0.0.0.0:9229",
        "start:dev:watch": "nodemon -L src/main.ts --inspect=0.0.0.0:9229 --exec ts-node",
        "lint": "eslint . --ext .ts",
        "format": "prettier --write \"src/**/*.+(js|jsx|ts|tsx|json)\"",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "keywords": [
        "typescript"
    ],
    "author": "Juanjofp",
    "license": "MIT",
    "devDependencies": {
        "@types/node": "^16.11.6",
        "@typescript-eslint/eslint-plugin": "^5.2.0",
        "@typescript-eslint/parser": "^5.2.0",
        "eslint": "^8.1.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-filenames": "^1.3.2",
        "nodemon": "^2.0.14",
        "prettier": "^2.4.1",
        "pretty-quick": "^3.1.1",
        "ts-node": "^10.4.0",
        "typescript": "^4.4.4"
    },
    "dependencies": {
        "@types/react": "^17.0.33",
        "react": "^17.0.2"
    }
}
